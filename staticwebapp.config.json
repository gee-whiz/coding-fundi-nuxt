{
  "routes": [
    {
      "route": "/article/id",
      "rewrite": "/article/id/index.html"
    },
    {
      "route": "/category/id",
      "rewrite": "/category/id/index.html"
    },
    {
      "route": "/index.html",
      "redirect": "/"
    },
    {
      "route": "/",
      "rewrite": "/api/server"
    }
